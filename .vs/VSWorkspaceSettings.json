{
  "BuildIncremental": {
    "Enabled": false
  },
  "ExcludedItems": [
    ".git*",
    ".clang*",
    ".idea",
    ".vscode",
    "*.md",
    "cpp.hint",
    "/scripts",
    "extension_api.json",
    "!.out*",
    "!.out/*",
    "!.out/build/*",
    "!README.md"
  ],
  "Indexing": {
    "Enabled": true
  },
  "Logger": {
    "EnableVsLogging": false,
    "LoggingLevel": "Info"
  }
}